{
    "jsc": {
        "parser": {
            "syntax": "typescript",
            "tsx": false,
            "decorators": false,
            "dynamicImport": false
        },
        "target": "es2018",
        "loose": false,
        "externalHelpers": false,
        "keepClassNames": true,
        "transform": {
            "hidden": {
                "jest": true
            }
        }
    },
    "module": {
        "type": "es6",
        "strict": true,
        "strictMode": true,
        "lazy": false,
        "noInterop": false
    },
    "minify": false
}